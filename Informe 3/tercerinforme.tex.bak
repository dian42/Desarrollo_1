\subsubsection{Diccionario de datos}
		\begin{enumerate}
			\item Rut = {[ 0 | 9 ]}
			\item Contraseña = {[A|Z $|$ a|z $|$ 0|9 $|$ \_ $|$ | $|$ . $|$ @ ]}
			\item Solicitud\_espacio = res\_fecha + res\_actividad + esp\_lugar.
			\item Confirmación\_reserva = ere\_id.
			\item Espacio\_común = esp\_lugar + esp\_descripcion + con\_nombre.
			\item Credencial\_acceso\_copropietario = Rut + Contraseña.
			\item Credencial\_acceso\_administrador = Rut + Contraseña.
			\item Ubicación\_imagen\_pago = ima\_path.
			\item Archivo\_excel\_gasto\_común =con\_nombre + tga\_nombre + gas\_descripcion + gas\_costo + gas\_fecha.
			\item Archivo\_excel\_gastos\_adicionales = pro\_num + tad\_nombre + adi\_descripcion + adi\_costo + adi\_fecha.
			\item Conjunto\_residencial = con\_nombre + con\_fono + con\_fondo + con\_direccion + tcon\_id + tcon\_descripcion.
			\item Datos\_personales = adm\_fono, adm\_mail, adm\_password.
			\item Dato\_copropietario = cop\_fono + cop\_direccion + cop\_mail + cop\_clave.
			\item Copropietario = cop\_rut + cop\_nombres + cop\_ap\_pat + cop\_ap\_mat + cop\_fono + cop\_mail cop\_direccion + cop\_contraseña + cop\_tipo + cop\_comuna.
			\item Propiedad = pro\_numero + pro\_m2 + pro\_alicuota + con\_id.
			\item Pago\_copropietario = ede\_id.
			\item Aviso\_reserva = res\_fecha + res\_actividad + ere\_id + esp\_id.
			\item Espacio\_disponible = fesp\_lugar + esp\_descripcion.
			\item Aviso\_solicitud\_reserva = res\_fecha + res\_actividad + pro\_numero + esp\_lugar + esp\_descripcion.
			\item Archivo\_excel\_gasto = con\_nombre + tga\_nombre.
			\item Archivo\_excel\_adicionales = pro\_num + tad\_nombre.
			\item Aviso\_pago = deu\_fecha + deu\_monto + deu\_fecha\_limite.
			\item Tablas\_básicas = Datos tablas (TIPO\_CONJUNTO, TIPO\_COPROPIETARIO, TIPO\_GASTO, TIPO\_ADICIONAL, ESTADO\_COPROPIETARIO, ESTADO\_DEUDA, ESTADO\_RESERVA, REGION, COMUNA)
			\item Reporte\_gasto\_copropietario = gas\_costo + gas\_descripcion + gas\_fecha + tga\_id + adi\_descripcion + adi\_costo + adi\_fecha, tad\_id.
			\item Reporte\_deuda\_copropietario = deu\_fecha + deu\_monto + deu\_fecha\_limite + deu\_ede\_id.
			\item Reporte\_histórico\_copropietario = deu\_fecha + deu\_monto + deu\_fecha\_limite + deu\_fecha\_cancel + deu\_ede\_id.
			\item Reporte\_morosos\_administrador = pro\_numero + ede\_id.
			\item Reporte\_gastos\_administrador = deu\_fecha + deu\_monto + deu\_fecha\_limite + deu\_fecha\_cancel + deu\_ede\_id, pro\_numero.
			\item Reporte\_estadístico\_administrador = Contador ere\_id + contador res\_fecha
			\item Reporte\_copropietario\_propiedad = pro\_numero + cop\_rut + cop\_nombres + cop\_ap\_pat + cop\_ap\_mat.
			\item Reporte\_deuda\_administrador = deu\_fecha + deu\_monto + deu\_fecha\_limite + deu\_ede\_id + pro\_numero.
			\item Identificador\_propiedad = pro\_id.
			\item Estado\_deuda = deu\_ede\_id.
			\item Gráfico\_estadístico\_copropietario = cpr\_pro\_id + adi\_descripcion + adi\_costo + gas\_descripcion + gas\_costo.
			\item Gráfico\_estadístico\_administrador = con\_nombre + cpr\_pro\_id + adi\_descripcion + adi\_costo + gas\_descripcion + gas\_costo.
	\end{enumerate}		
